{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["@repo/db#build", "@repo/dto#build", "@repo/ui#build", "@repo/web-utils#build"]
    },
    "client": {
      "dependsOn": ["@repo/dto#build", "@repo/ui#build", "@repo/web-utils#build"]
    },
    "@repo/web-utils#build": {
      "dependsOn": []
    },
    "@repo/dto#buils": {
      "dependsOn": ["@repo/db#build"]
    },
    "@repo/db#build": {
      "dependsOn": []
    },
    "@repo/ui#build": {
      "dependsOn": ["@repo/web-utils#build", "@repo/dto#build"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
