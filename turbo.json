{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "client": {
      "dependsOn": ["@repo/dto#build", "@repo/ui#build", "@repo/web-utils#build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "@repo/web-utils#build": {
      "dependsOn": [],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**"]
    },
    "@repo/dto#buils": {
      "dependsOn": ["@repo/db#build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "@repo/db#build": {
      "dependsOn": [],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**", "!.prisma/**"]
    },
    "@repo/ui#build": {
      "dependsOn": ["@repo/web-utils#build", "@repo/dto#build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
